{
  "name": "esnext-scripts",
  "version": "1.1.0",
  "description": "Opinionated ESNext application scripts and configurations",
  "main": "cmd.js",
  "bin": {
    "esnext": "./cmd.js",
    "esnext-scripts": "./cmd.js"
  },
  "directories": {
    "lib": "lib"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "commit": "git-cz",
    "format": "cross-env DEBUG=esnext-scripts ./cmd.js format './**/*.js'",
    "lint": "cross-env DEBUG=esnext-scripts ./cmd.js lint",
    "test": "cross-env DEBUG=esnext-scripts ./cmd.js test",
    "prerelease": "standard-version",
    "release": "git push --follow-tags origin master && npm publish --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/esnext-scripts.git"
  },
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/esnext-scripts/issues"
  },
  "homepage": "https://github.com/jimzhan/esnext-scripts#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "cross-env": "^5.2.0",
    "debug": "^4.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "execa": "^1.0.0",
    "jest": "^24.5.0",
    "jest-cli": "^24.5.0",
    "log-symbols": "^2.2.0",
    "nodemon": "^1.18.10",
    "prettier-standard": "^9.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "standard-version": "^5.0.1"
  },
  "keywords": [
    "6to5",
    "zero-configuration",
    "babel",
    "babel7",
    "babel-core",
    "boilerplate",
    "build",
    "build-tools",
    "const",
    "compiler",
    "classes",
    "enzyme",
    "esnext",
    "es6",
    "es7",
    "es8",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "eslint",
    "generator",
    "harmony",
    "let",
    "module",
    "application",
    "app",
    "jest",
    "react",
    "skeleton",
    "template",
    "test",
    "transpile",
    "transpiler",
    "lint",
    "var"
  ]
}
